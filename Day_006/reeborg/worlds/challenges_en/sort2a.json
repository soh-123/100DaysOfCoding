  "onload": [
    "var col, nb_cols, piles, height;",
    "",
    "nb_cols = RUR.randint(3, 9);",
    "piles = [];",
    "",
    "for (col=1; col <= nb_cols; col++) {",
    "    height = RUR.randint(1, 9);",
    "    piles.push(height);",
    "    RUR.add_object('token', col, 1, {number:height});",
    "}",
    "piles.sort(function(a, b) {",
    "    return a - b;",
    "});",
    "",
    "for (col=1; col <= nb_cols; col++) {",
    "    height = piles[col-1];",
    "    RUR.add_object('token', col, 1, {goal:true, number:height});",
    "}",

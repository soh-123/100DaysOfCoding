{
  "robots": [],
  "walls": {},
  "description": [
    "리보그는 시골로 가서 등산하는것을 좋아해요. 한 가지 문제점이 있다면, 가끔 쉽게 길을 잃는다는 것이에요. 리보그가 조심히 등산로를 따라가고 결과적으로 등산로를 끝가지 갈 수 있도록 프로그램을 만드세요."
  ],
  "small_tiles": false,
  "rows": 12,
  "cols": 14,
  "onload": [
    "var height, width, choice, path, start_x, start_y, finish;",
    "",
    "width = 14;",
    "height = 14;",
    "path = [];",
    "choice = RUR.randint(0, 2);",
    "",
    "RUR.set_world_size(width, height);",
    "RUR.fill_background(\"grass\");",
    "",
    "if (choice === 0) {",
    "  start_x = 2;",
    "  start_y = 4;",
    "  path = [[2, 4], [3, 4], [4, 4], [4, 5], [4, 6], [4, 7], [5, 7], [6, 7], [7, 7], [7, 6], [7, 5], [8, 5], [9, 5], [10, 5], [11, 5], [11, 6], [11, 7], [11, 8], [11, 9], [11, 10], [11, 11], [11, 12]];",
    "  finish = [11, 12]; // should begin by facing right - no change needed",
    "  r = new UsedRobot(start_x, start_y, 'e');",
    "} else if (choice == 1) {",
    "  start_x = 5;",
    "  start_y = 7;",
    "  path = [[5, 7], [5, 8], [5, 9], [6, 9], [7, 9], [8, 9], [8, 8], [8, 7], [8, 6], [8, 5], [8, 4], [7, 4], [6, 4], [5, 4], [4, 4], [3, 4], [2, 4], [2, 5], [2, 6], [2, 7], [2, 8], [2, 9], [2, 10], [2, 11], [2, 12], [3, 12], [4, 12], [5, 12], [6, 12], [7, 12], [8, 12], [9, 12], [10, 12], [11, 12], [12, 12]];",
    "  finish = [12, 12]; // should begin by facing north",
    "  r = new UsedRobot(start_x, start_y, 'n');",
    "} else if (choice === 2) {",
    "  start_x = 7;",
    "  start_y = 9;",
    "  path = [[7, 9], [6, 9], [5, 9], [5, 8], [5, 7], [5, 6], [5, 5], [6, 5], [7, 5], [8, 5], [9, 5], [10, 5], [11, 5], [12, 5], [12, 6], [12, 7], [12, 8], [12, 9], [11, 9], [10, 9]];",
    "  finish = [10, 9]; //should begin by facing west",
    "  r = new UsedRobot(start_x, start_y, 'w');",
    "}",
    "",
    "for (let i=0; i<path.length;i++) {",
    "    RUR.add_object(\"square\", path[i][0], path[i][1]);",
    "}",
    ""
  ]
}

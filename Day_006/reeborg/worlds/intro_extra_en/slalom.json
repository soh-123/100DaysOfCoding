  "onload": [
    "RUR.frame_insertion = function (name, arg) {",
    "    var x, y;",
    "    if (name != \"move\") {",
    "        return;",
    "    }",
    "    robot = RUR.get_robot_by_id(arg);",
    "    x = robot.body.x;",
    "    y = robot.body.y;",
    "    if (RUR.is_background_tile(\"ice\", x, y)) {",
    "        RUR.record_frame(name, arg);",
    "        while (RUR.is_background_tile(\"ice\", x, y)) {",
    "            RUR._write_ln(\"The ice is too slippery!\");",
    "            RUR._move_();",
    "        }",
    "    }",
    "}"
